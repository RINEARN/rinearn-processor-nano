float a = 2.0;
float b = -1.0;
float c = 3.0;

float r = 1.01;

float f(float x) {
	return a*x*x + b*x + c;
}

float g(float x) {
	if (x < 0) {
		return 0;
	}
	float value = x;
	for (int i=0; i<10; i++) {
		value *= r;
	}
	return value;
}

